<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a_level">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Alevel"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".mypage.ChangePasswordActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.ChangeNicknameActivity"
            android:exported="false" />
        <activity
            android:name=".common.MainActivity"
            android:exported="true" />
        <activity
            android:name=".keyword.UserStyleActivity"
            android:exported="false" />
        <activity
            android:name=".keyword.UserKeywordActivity"
            android:exported="false" />
        <activity
            android:name=".login.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".login.LoginActivity"
            android:exported="false"
            android:parentActivityName=".login.StartActivity" />
        <activity
            android:name=".login.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mypage.MyAccountActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.ScrapActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.WrittenPostActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.WrittenCommentActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.SettingActivity"
            android:exported="false" />
        <activity
            android:name=".mypage.AddAlcoholActivity"
            android:exported="false" />
        <activity android:name=".allalcohol.AllAlcoholSearchActivity" />
        <activity android:name=".common.AlcoholDetailActivity" />
        <activity android:name=".feed.FeedDetailActivity" />
        <activity
            android:name=".common.AlcoholDetailWriteReviewActivity"
            android:theme="@style/Theme.Alevel.ActionBar" />
        <activity android:name=".feed.WriteFeedActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>