<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp">

    <ImageView
        android:id="@+id/imageview_feeditem"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/common_roundrectangle"
        android:clipToOutline="true"
        android:scaleType="centerCrop"
        android:src="@drawable/common_alcohol_test"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textview_feeditem_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="게시글 제목"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageview_feeditem" />

    <TextView
        android:id="@+id/textview_feeditem_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="게시글 내용"
        android:textColor="#6A6A6A"
        android:textSize="14dp"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_title" />

    <TextView
        android:id="@+id/textview_feeditem_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="주종"
        android:textColor="@color/gray5"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_dbtype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/feeditemrecyclerview_textview_border"
        android:paddingStart="9dp"
        android:paddingTop="1dp"
        android:paddingEnd="9dp"
        android:paddingBottom="1dp"
        android:text="주종"
        android:textColor="@color/primary"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_type"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="11dp"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="이름"
        android:textColor="@color/gray5"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_dbtype"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_dbname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/feeditemrecyclerview_textview_border"
        android:paddingStart="9dp"
        android:paddingTop="1dp"
        android:paddingEnd="9dp"
        android:paddingBottom="1dp"
        android:text="이름"
        android:textColor="@color/primary"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_name"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="11dp"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:text="도수"
        android:textColor="@color/gray5"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_dbname"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_dbvolume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/feeditemrecyclerview_textview_border"
        android:paddingStart="9dp"
        android:paddingTop="1dp"
        android:paddingEnd="9dp"
        android:paddingBottom="1dp"
        android:text="도수"
        android:textColor="@color/primary"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_volume"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_content" />

    <TextView
        android:id="@+id/textview_feeditem_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginVertical="26dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="16dp"
        android:text="0000.00.00"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_type" />

    <ImageView
        android:id="@+id/imageview_feeditem_comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:src="@drawable/feed_icon_comment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_date"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_type" />

    <TextView
        android:id="@+id/textview_feeditem_commentcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="댓글 0"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageview_feeditem_comment"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_type" />

    <ImageView
        android:id="@+id/imageview_feeditem_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:src="@drawable/feed_icon_like"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textview_feeditem_commentcount"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_type" />

    <TextView
        android:id="@+id/textview_feeditem_likecount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="짠 0"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageview_feeditem_like"
        app:layout_constraintTop_toBottomOf="@+id/textview_feeditem_type" />


    <View
        android:id="@+id/view_feeditem"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray2"
        app:layout_constraintBottom_toBottomOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>